{
  "name": "@creativoma/numeric-background",
  "version": "1.0.7",
  "type": "module",
  "description": "A React component for creating a numeric background",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/creativoma/numeric-background#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativoma/numeric-background.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build:demo": "vite build --config vite.config.ts",
    "build:lib": "tsc && vite build --config vite.config.lib.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build:lib"
  },
  "keywords": [
    "react",
    "background",
    "numeric",
    "component",
    "animation",
    "ui",
    "typescript",
    "frontend"
  ],
  "author": {
    "name": "creativoma"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^20.19.25",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
